{
  "sourceRoot": "packages/nestjs/twilio/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npx nx run nestjs-twilio:build-base",
        "color": true
      }
    },
    "build-base": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nestjs/twilio",
        "tsConfig": "packages/nestjs/twilio/tsconfig.lib.json",
        "packageJson": "packages/nestjs/twilio/package.json",
        "main": "packages/nestjs/twilio/src/index.ts",
        "assets": ["packages/nestjs/twilio/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/nestjs/twilio/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/nestjs/twilio"],
      "options": {
        "jestConfig": "packages/nestjs/twilio/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
