{
  "extends": ["@commitlint/config-angular"],
  "rules": {
    "type-enum": [
      2,
      "always",
      [
        "merge",
        "build",
        "ci",
        "docs",
        "feat",
        "minor",
        "fix",
        "perf",
        "refactor",
        "style",
        "test",
        "release",
        "demo",
        "checkpoint"
      ]
    ]
  }
}
