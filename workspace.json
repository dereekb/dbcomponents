{
  "version": 2,
  "projects": {
    "browser": "packages/browser",
    "date": "packages/date",
    "dbx-core": "packages/dbx-core",
    "dbx-form": "packages/dbx-form",
    "dbx-web": "packages/dbx-web",
    "model": "packages/model",
    "ngx-analytics": "packages/ngx-analytics",
    "rxjs": "packages/rxjs",
    "util": "packages/util",
    "workspace": {
      "root": ".",
      "targets": {
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": true,
            "commitMessageFormat": "chore(${projectName}): release version ${version}"
          }
        }
      }
    }
  }
}
