          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "packages/demo-api/src/environments/environment.ts",
                  "with": "packages/demo-api/src/environments/environment.prod.ts"
                }
              ]
