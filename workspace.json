    "browser": "packages/browser",
    "date": {
      "root": "packages/date",
      "sourceRoot": "packages/date/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/date",
            "tsConfig": "packages/date/tsconfig.lib.json",
            "packageJson": "packages/date/package.json",
            "main": "packages/date/src/index.ts",
            "assets": ["packages/date/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/date/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/date", "./.reports/jest/date.junit.xml"],
          "options": {
            "jestConfig": "packages/date/jest.config.js",
            "passWithNoTests": true
          }
        },
        "publish-npmjs": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "cd ./dist/packages/date && npm publish"
          }
        }
      },
      "tags": []
    },    
