        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx nx build demo-api"
              },
              {
                "command": "npx nx deploy-dist-to-functions demo-api"
              }
            ],
            "parallel": false
          }
        }
      },
      "tags": []
    },
