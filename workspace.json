{
  "version": 2,
  "projects": {
    "browser": "packages/browser",
    "date": "packages/date",
    "dbx-analytics": {
      "projectType": "library",
      "root": "packages/dbx-analytics",
      "sourceRoot": "packages/dbx-analytics/src",
      "prefix": "dereekb",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "outputs": ["dist/packages/dbx-analytics"],
          "options": {
            "project": "packages/dbx-analytics/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/dbx-analytics/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/dbx-analytics/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/dbx-analytics"],
          "options": {
            "jestConfig": "packages/dbx-analytics/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/dbx-analytics/src/**/*.ts",
              "packages/dbx-analytics/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "dbx-core": "packages/dbx-core",
    "dbx-firebase": {
      "projectType": "library",
      "root": "packages/dbx-firebase",
      "sourceRoot": "packages/dbx-firebase/src",
      "prefix": "dereekb",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "outputs": ["dist/packages/dbx-firebase"],
          "options": {
            "project": "packages/dbx-firebase/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/dbx-firebase/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/dbx-firebase/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/dbx-firebase"],
          "options": {
            "jestConfig": "packages/dbx-firebase/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/dbx-firebase/src/**/*.ts",
              "packages/dbx-firebase/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "dbx-form": "packages/dbx-form",
    "dbx-web": "packages/dbx-web",
    "demo": {
      "projectType": "application",
      "root": "packages/demo",
      "sourceRoot": "packages/demo/src",
      "prefix": "dereekb",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/demo",
            "index": "packages/demo/src/index.html",
            "main": "packages/demo/src/main.ts",
            "polyfills": "packages/demo/src/polyfills.ts",
            "tsConfig": "packages/demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "packages/demo/src/favicon.ico",
              "packages/demo/src/assets"
            ],
            "styles": ["packages/demo/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "packages/demo/src/environments/environment.ts",
                  "with": "packages/demo/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            },
            "development": {
              "browserTarget": "demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/demo/src/**/*.ts",
              "packages/demo/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/demo"],
          "options": {
            "jestConfig": "packages/demo/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy-dist-to-hosting": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "firebase deploy --only hosting"
          }
        },
        "deploy": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "nx build demo"
              },
              {
                "command": "nx deploy-dist-to-hosting demo"
              }
            ],
            "parallel": false
          }
        }
      },
      "tags": []
    },
    "demo-api": {
      "root": "packages/demo-api",
      "sourceRoot": "packages/demo-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/demo-api",
            "main": "packages/demo-api/src/main.ts",
            "tsConfig": "packages/demo-api/tsconfig.app.json",
            "assets": [
              "packages/demo-api/src/assets"
            ],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "packages/demo-api/src/environments/environment.ts",
                  "with": "packages/demo-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "run-emulators": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "firebase emulators:start"
          }
        },
        "serve-api": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "demo-api:build"
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "nx build demo-api --watch"
              },
              {
                "command": "nx run-emulators demo-api"
              }
            ],
            "parallel": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/demo-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/demo-api"],
          "options": {
            "jestConfig": "packages/demo-api/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy-dist-to-functions": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "firebase deploy --only functions"
          }
        },
        "deploy": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "nx build demo-api"
              },
              {
                "command": "nx deploy-dist-to-functions demo-api"
              }
            ],
            "parallel": false
          }
        }
      },
      "tags": []
    },
    "demo-e2e": {
      "root": "packages/demo-e2e",
      "sourceRoot": "packages/demo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/demo-e2e/cypress.json",
            "devServerTarget": "demo:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/demo-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["demo"]
    },
    "model": "packages/model",
    "rxjs": "packages/rxjs",
    "util": "packages/util",
    "workspace": {
      "root": ".",
      "targets": {
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": true,
            "commitMessageFormat": "chore(${projectName}): release version ${version}"
          }
        }
      }
    }
  }
}
